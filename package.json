    "phantomjs-prebuilt": "^2.1.4",
    "shell-escape": "^0.2.0",
    "grunt-sass-lint": "^0.2.0"
  },
  "browser": {
    "bootstrap": "./assets/vendor/bootstrap-sass-twbs/assets/javascripts/bootstrap.js",
