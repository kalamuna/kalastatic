{
  "name": "kalastatic",
  "private": true,
  "dependencies": {
    "bower": "~1.3.12",
    "metalsmith": "~1.0.1",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-collections": "^0.6.0",
    "metalsmith-concat": "^1.0.0",
    "metalsmith-markdown": "~0.2.1",
    "metalsmith-permalinks": "~0.4.0",
    "metalsmith-sass": "git://github.com/RobLoach/metalsmith-sass.git#fix-buffer",
    "metalsmith-swig-helpers": "^0.1.0",
    "metalsmith-templates": "~0.6.0",
    "swig": "^1.4.2"
